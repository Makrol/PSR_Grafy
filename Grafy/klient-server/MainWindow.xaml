<Window x:Class="Grafy_serwer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Grafy_serwer"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="854">
    <TabControl x:Name="mainTabControl" SelectedIndex="-1">
        <TabItem Visibility="Collapsed">
            <Frame Source="./Pages/WelcomePage.xaml"/>
        </TabItem>
        <TabItem Visibility="Collapsed">
            <Frame Source="./Pages/GraphEditorPage.xaml"/>
        </TabItem>
        <TabItem Visibility="Collapsed">
            <Frame Source="./Pages/ConnectionPage.xaml"/>
        </TabItem>
        <TabItem Visibility="Collapsed">
            <Frame Source="./Pages/ServerPage.xaml"/>
        </TabItem>
        <TabItem Visibility="Collapsed">
            <Frame Source="./Pages/ClientPage.xaml"/>
        </TabItem>
    </TabControl>
</Window>
